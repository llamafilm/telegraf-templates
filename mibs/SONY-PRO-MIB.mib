::= { professionalV1 2 }

--
-- OBJECT IDENTITY
--

proV1ProductInfo OBJECT-IDENTITY
	STATUS  current
	DESCRIPTION
		"Product ID Information comprises the ID information table for specifying
		the product and the module information table for acquiring the information
		for the modules included in the product."
	-- 1.3.6.1.4.1.122.8501.2.1
::= { proV1Common 1 }

--
-- Product Information
--

proV1ProdIdInfoTable OBJECT-TYPE
	SYNTAX  SEQUENCE OF ProV1ProdIdInfoEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"Stores the Product ID Information in table format."
	-- 1.3.6.1.4.1.122.8501.2.1.1
::= { proV1ProductInfo 1 }


proV1ProdIdInfoEntry OBJECT-TYPE
	SYNTAX  ProV1ProdIdInfoEntry
	MAX-ACCESS not-accessible
	STATUS  current
	DESCRIPTION
		"An entry in the proV1ProdIdInfoTable."
	INDEX {
		proV1ProdIdInfoIdx }
	-- 1.3.6.1.4.1.122.8501.2.1.1.1
::= { proV1ProdIdInfoTable 1 }


ProV1ProdIdInfoEntry ::= SEQUENCE {

	proV1ProdIdInfoIdx         Integer32,
	proV1ProdIdInfoDevID       DisplayString,
	proV1ProdIdInfoManufact    DisplayString,
	proV1ProdIdInfoModel       DisplayString,
	proV1ProdIdInfoDest        DisplayString,
	proV1ProdIdInfoSN          DisplayString,
	proV1ProdIdInfoCat         INTEGER,
	proV1ProdIdInfoVer         Integer32,
	proV1ProdIdInfoModelSuffix DisplayString,
	proV1ProdIdInfoVerSuffix   DisplayString }


proV1ProdIdInfoIdx OBJECT-TYPE
	SYNTAX  Integer32 (1..2147483647)
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"This is an index number for specifying the entries in this table."
	-- 1.3.6.1.4.1.122.8501.2.1.1.1.1
::= { proV1ProdIdInfoEntry 1 }


proV1ProdIdInfoDevID OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..255))
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"A device number or name is entered that is set so that the customer
		can identify the product. It is assumed that the device ID is either
		set numerically from the control panel of the product or that
		a character string is set using http or telnet through the network.
		Also, it is mounted so that the Device ID in the first line (instance)
		of this table takes the same value as sysName in the MIB-2 System Group."
	-- 1.3.6.1.4.1.122.8501.2.1.1.1.2
::= { proV1ProdIdInfoEntry 2 }


proV1ProdIdInfoManufact OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..64))
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The vendor name is entered."
	-- 1.3.6.1.4.1.122.8501.2.1.1.1.3
::= { proV1ProdIdInfoEntry 3 }


proV1ProdIdInfoModel OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..255))
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The model name is entered. Also, it is mounted so that the Model Name
		in the first line (instance) of this table takes the same value as
		sysDescr in the MIB-2 System group."
	-- 1.3.6.1.4.1.122.8501.2.1.1.1.4
::= { proV1ProdIdInfoEntry 4 }


proV1ProdIdInfoDest OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..64))
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The destination is entered.
		(Examples: J: For Japan, UC: For North American, EK: For European Countries, SY: For World Wide)"
	-- 1.3.6.1.4.1.122.8501.2.1.1.1.5
::= { proV1ProdIdInfoEntry 5 }


proV1ProdIdInfoSN OBJECT-TYPE
	SYNTAX  DisplayString (SIZE (0..64))
	MAX-ACCESS read-only
	STATUS  current
	DESCRIPTION
		"The serial number is entered."
	-- 1.3.6.1.4.1.122.8501.2.1.1.1.6
::= { proV1ProdIdInfoEntry 6 }


